{
    "name": "pbd-kn/contao-contaohab-bundle",
    "description": "Bundle zum Manager von Home Administration",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Peter Broghammer",
            "email": "pb-contao@gmx.de",
            "homepage": "https://pb-broghammer.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pbd-kn/contao-contaohab-bundle/issues",
        "source": "https://github.com/pbd-kn/contao-contaohab-bundle"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.12",
        "symplify/easy-coding-standard": "11.1"
    },
    "autoload": {
        "psr-4": {
            "PbdKn\\ContaoContaohabBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": false,
            "contao/manager-plugin": false,
            "contao-community-alliance/composer-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "contao-manager-plugin": "PbdKn\\ContaoContaohabBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "vendor\\bin\\ecs.bat check src\\ tests\\ --fix --ansi",
        "unit-tests": "vendor\\bin\\phpunit.bat"
    },
    "version": "1.0.1"
}
